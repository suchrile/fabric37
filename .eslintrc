{
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "semi": [2, "always"],
    "vue/no-v-html": 0,
    "vue/multi-word-component-names": 0
  }
}
